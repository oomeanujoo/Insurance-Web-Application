.header {
  height: 61px;
  background: linear-gradient(to bottom, #f58220 -18%, #b02a30 127%);
}
.logoWrapper {
  display: inline-block;
  float: left;
  height: 50px;
}
.logo {
  height: 30px;
  margin: 1rem;
  width: unset;
  cursor: pointer;
}
.headerTitle {
  color: #fff;
  font-size: 16px;
  padding: 13px 10px;
  float: left;
  font-weight: 600;
}
.content-container {
  height: calc(100vh - 50px);
  background: #fff;
  width: 100%;
}

.content-container {
  height: calc(100vh - 50px);
  background: #fff;
}
.content-view {
  overflow-y: scroll;
  /* padding-top: 20px; */
  height: calc(100vh - 50px);
  padding: 0px 5px;
  /* ankit edit */
  /* padding-left: 64px; */
}
.content {
  /* width: calc(100vw - 230px); */
}
@media screen and (max-width: 575px) {
  .content {
    width: 100%;
  }
}

@media (min-width: 600px) {
  .sidenav {
    /* position: relative !important; */
    float: left;
    height: 100%;
    display: block;
    padding-top: 0px;
    overflow-y: scroll;
  }
  .content {
    /* margin-left: 230px */
  }
  .menu-btn {
    text-align: left;
  }
}
.widthFull {
  width: 100%;
}
.menu-btn {
  text-transform: none;
  font-size: 14px;
  font-weight: normal;
  border: none;
}
.menu-heading {
  background: #ff0000;
  color: #fff;
  font-weight: 500;
  padding: 7px 10px;
  max-height: 33px;
  display: inline-block;
}
.menuButtonWithLoader {
  border: 1px solid #ff0000;
  border-top: none;
}

label {
  margin: 0px;
}

.btn-subMenu {
  background: #fff;
  color: #ff0000;
  border: none;
  border-radius: 0px;
  transition: 0.4s;
  font-weight: 500;
  max-height: 30px;
}
.btn-subMenu:hover {
  /*box-shadow: inset 0px 8px 12px -8px, inset 0px -8px 12px -8px;
        transition: all 0.4s ease;
        transform: scale(1.1);
        box-shadow: 1px 1px 1px 1px;*/
  background: darkslategray !important;
  color: #fff;
  box-shadow: 0px 0px 0px 0px;
}

.active-menu .menu-btn {
  background: darkslategray;
  color: #fff;
  box-shadow: 0px 0px 0px 0px;
}

.headerSmall {
  display: none;
}
.headerBig {
  display: inline-block;
}
@media (max-width: 600px) {
  .headerSmall {
    display: inline-block;
  }
  .headerBig {
    display: none;
  }
  .content {
    width: 100%;
  }
}
.menuIcon {
  cursor: pointer;
}

.contentFixed {
  padding-left: 0px;
}
.sidenavFixed {
  position: relative !important;
}
.closeMenuItem {
  border: none;
  padding-left: 0px;
  padding-right: 0px;
  transition: padding 0.5s;
}


