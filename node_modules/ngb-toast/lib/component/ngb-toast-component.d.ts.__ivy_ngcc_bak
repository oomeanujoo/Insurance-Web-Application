import { NgbToastService } from '../provider/ngb-toast.service';
export declare class NgbToastComponent implements OnInit {
    private ngbToastService;
    toast: NgbToast;
    isHidden: boolean;
    constructor(ngbToastService: NgbToastService);
    ngOnInit(): void;
    dismiss(): void;
}
